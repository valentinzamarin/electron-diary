    "electron-rebuild": "^3.2.9",
    "vite": "^6.2.2",
    "vite-plugin-electron": "^0.29.0",
    "vue": "^3.5.13"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.14",
    "@tiptap/starter-kit": "^2.11.5",
    "@tiptap/vue-3": "^2.11.5",
    "sqlite3": "^5.1.7",
    "vue-router": "^4.5.0"
  }
